{
    "name": "benyaminrmb/laravel-dynamic-resources",
    "description": "creating dynamic API resources",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0@dev"
    },

    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Benyaminrmb\\LaravelDynamicResources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benyaminrmb\\LaravelDynamicResources\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Benyamin Rmb",
            "email": "benyaminrmb@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
